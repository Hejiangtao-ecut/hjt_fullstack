page {
  display: flex;
  flex-direction: column;
  height: 100%;
}
.map-box {
  width: 100%;
  height: 100%;
  overflow: hidden;
  flex: 1;
}
.main-btn {
  background-color: #fff;
  padding: 20rpx;
}
.main-text {
  height: 100rpx;
  margin: 0 auto;
  color:#fff;
  background-color: #4a4c5b;
  font-size: 32rpx;
  border-radius: 4rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
}